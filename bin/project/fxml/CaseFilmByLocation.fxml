<AnchorPane id="AnchorPane" prefHeight="385.0" prefWidth="600.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.CaseFilmByLocationController">
   <children>
      <VBox alignment="BOTTOM_RIGHT" layoutX="1.0" prefHeight="385.0" prefWidth="600.0">
         <children>
            <VBox alignment="CENTER_LEFT" prefHeight="284.0" prefWidth="480.0">
               <children>
                  <Label text="Enter the article's title" />
                  <TextField fx:id="location" promptText="Enter the article's title">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </TextField>
                  <Label text="Results">
                     <VBox.margin>
                        <Insets top="43.0" />
                     </VBox.margin>
                  </Label>
                  <TextArea fx:id="ta" prefHeight="117.0" prefWidth="200.0">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </TextArea>
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <VBox.margin>
                  <Insets left="60.0" right="60.0" />
               </VBox.margin>
            </VBox>
            <HBox alignment="TOP_RIGHT" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button alignment="TOP_RIGHT" mnemonicParsing="false" onAction="#changeScreenButtonPushed" prefHeight="33.0" prefWidth="166.0" text="Go Back to menu">
                     <HBox.margin>
                        <Insets right="16.0" top="20.0" />
                     </HBox.margin>
                  </Button>
                  <Button alignment="TOP_RIGHT" mnemonicParsing="false" onAction="#getPushed" prefHeight="33.0" prefWidth="75.0" text="Get">
                     <HBox.margin>
                        <Insets right="60.0" top="20.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
